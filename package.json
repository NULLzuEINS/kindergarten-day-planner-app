{
  "name": "dayplanner-app",
  "description": "Tagesplaner",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:assets": "./node_modules/.bin/pwa-asset-generator public/img/icon.png -i public/index.html -m public/manifest.json -v '<%= BASE_URL %>/img' -q 85 --dark-mode --opaque public/img/icons",
    "proxy": " ./node_modules/.bin/ngrok http 8080 -host-header='localhost:8080'",
    "proxy:dist": " ./node_modules/.bin/ngrok http file://$PWD/dist"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "fs": "^0.0.1-security",
    "jspdf": "^2.3.1",
    "register-service-worker": "^1.7.1",
    "vue": "^3.0.1",
    "vue-loader": "15.9.8",
    "vue-toast-notification": "^2.0",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "ngrok": "^4.2.2",
    "pwa-asset-generator": "^4.3.9"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": ">=14 <=16.1.0"
  }
}
